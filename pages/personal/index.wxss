/**index.wxss**/
.container{
  width: 100%;
  height: 100vh;
  background-color: red;
  display: flex;
  justify-content: center;
  align-items: center;
}
.container .cart_route{
  padding: 10px 10px;
  width: calc(80% - 22px);
  box-shadow: 1px 2px 1px #dddddd;
  background-color: white;
  border-radius: 10px;
}

.cart_route_bottom{
  padding: 10px 0px;
  border-bottom: 1px solid #eeeeee;
}
.cart_route_top{
  padding-bottom: 10px;
  border-bottom: 1px solid #eeeeee;
}
.cart_route_bot{
  padding-top: 10px;
  border-bottom: 0px solid #eeeeee;
}